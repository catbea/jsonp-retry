{
  "presets": [
    ["es2015", {
      "modules": false,
      "loose": false
    }],
    "stage-0"
  ],
  "env": {
    "test": {
      "presets": ["es2015", "stage-0"]
    }
  },
  "plugins": [
    "transform-es3-member-expression-literals",
    "transform-es3-property-literals"
  ],
  "comments": false
}
